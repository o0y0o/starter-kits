{
  "name": "@0y0/starter-kits",
  "version": "1.0.2",
  "description": "Dev starter kits.",
  "repository": "https://github.com/o0y0o/starter-kits",
  "author": "Jason Chung",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "gen": "script/gen.js"
  },
  "files": [
    "/_templates",
    "/script"
  ],
  "keywords": [
    "hygen"
  ],
  "scripts": {
    "format": "prettier --write 'script/**/*.js'",
    "pkg": "[ $(yarn info $npm_package_name version) != $npm_package_version ] && yarn publish || echo Skip publishing due to v$npm_package_version exist"
  },
  "dependencies": {
    "hygen": "^6.0.4"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  }
}
